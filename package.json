{
  "name": "@parcel/ssr-example",
  "version": "2.0.0-beta.3.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build": "parcel build .",
    "serve": "NODE_ENV=production node server.js"
  },
  "client": "dist-client/index.html",
  "ssr": "dist-server/index.js",
  "targets": {
    "client": {
      "source": "src/index.html",
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    },
    "ssr": {
      "source": "src/index.server.jsx",
      "isLibrary": true,
      "engines": {
        "node": ">= 14.0.0"
      }
    }
  },
  "devDependencies": {
    "@parcel/config-default": "2.0.0-nightly.722",
    "@parcel/core": "2.0.0-nightly.720",
    "@parcel/resolver-glob": "2.0.0-nightly.2344",
    "express": "^4.17.1",
    "parcel": "2.0.0-nightly.720",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0",
    "serve-static": "^1.14.1"
  },
  "dependencies": {
    "@parcel/reporter-cli": "2.0.0-nightly.722",
    "import-fresh": "^3.3.0"
  }
}
